{
    "version": 2,
    "builds": [
      {
        "src": "Myapi/app.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/persons/([^/]+)",
        "dest": "/api/persons?id=$1"
      },
      {
        "src": "/api/persons/([^/]+)/?$",
        "dest": "/api/persons?name=$1"
      },
      {
        "src": "/api/persons",
        "dest": "/api/persons"
      }
    ]
  }
  